            app:speed_limit="4"
            app:speedTextSize="10dp"
            app:unitOfMeasurementTextSize="3dp"
            app:speedLimitTextSize="7dp"
            app:speedDialRingWidth="7dp"
            app:speedDialRingInnerPadding="7dp"
            app:maxSpeed="10"
            app:animationTime="5000"
            android:layout_marginTop="8dp"
            app:layout_constraintTop_toBottomOf="@+id/textView5"
            app:layout_constraintStart_toStartOf="@+id/smRV"/>
    <in.unicodelabs.kdgaugeview.KdGaugeView
            android:id="@+id/smBC"
            android:layout_width="100dp"
            android:layout_height="100dp"
            android:layout_gravity="center"
            app:speed_limit="4"
            app:speedTextSize="10dp"
            app:unitOfMeasurementTextSize="3dp"
            app:speedLimitTextSize="7dp"
            app:speedDialRingWidth="7dp"
            app:animationTime="5000"
            app:speedDialRingInnerPadding="7dp"
            app:maxSpeed="10"
            app:layout_constraintTop_toTopOf="@+id/smRC" app:layout_constraintStart_toEndOf="@+id/smRC"
            android:layout_marginStart="8dp" android:layout_marginEnd="8dp"
            app:layout_constraintEnd_toStartOf="@+id/smYC"/>
    <in.unicodelabs.kdgaugeview.KdGaugeView
            android:id="@+id/smYC"
            android:layout_width="100dp"
            android:layout_height="100dp"
            android:layout_gravity="center"
            app:speed_limit="4"
            app:speedTextSize="10dp"
            app:unitOfMeasurementTextSize="3dp"
            app:speedLimitTextSize="7dp"
            app:animationTime="5000"
            app:speedDialRingWidth="7dp"
            app:speedDialRingInnerPadding="7dp"
            app:maxSpeed="10"
            app:unitOfMeasurement=""
            app:layout_constraintTop_toTopOf="@+id/smBC"
            android:layout_marginEnd="8dp"
            app:layout_constraintEnd_toEndOf="@+id/smYV"/>
    <TextView
            android:text="V"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/textView5"
            app:layout_constraintStart_toStartOf="@+id/smRV" android:layout_marginStart="8dp"
            android:layout_marginTop="8dp" app:layout_constraintTop_toBottomOf="@+id/smBV"
            app:layout_constraintEnd_toEndOf="@+id/smYV" android:layout_marginEnd="8dp"/>
    <TextView
            android:text="A"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/textView6" android:layout_marginTop="8dp"
            app:layout_constraintTop_toBottomOf="@+id/smBC" app:layout_constraintEnd_toEndOf="parent"
            android:layout_marginEnd="8dp" app:layout_constraintStart_toStartOf="parent"
            android:layout_marginStart="8dp"/>
    <TextView
            android:text="KWH"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/textView7" android:layout_marginTop="8dp"
            app:layout_constraintTop_toBottomOf="@+id/smKVA2" app:layout_constraintEnd_toEndOf="parent"
            android:layout_marginEnd="8dp" app:layout_constraintStart_toStartOf="parent"
            android:layout_marginStart="8dp"/>


