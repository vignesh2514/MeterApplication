            app:layout_constraintTop_toTopOf="@+id/smKVA2"
            app:layout_constraintEnd_toEndOf="@+id/smYC"/>
    <in.unicodelabs.kdgaugeview.KdGaugeView
            android:id="@+id/smKVA1"
            android:layout_width="100dp"
            android:layout_height="100dp"
            android:layout_gravity="center"
            app:speed_limit="4"
            app:animationTime="5000"
            app:speedTextSize="10dp"
            app:unitOfMeasurementTextSize="3dp"
            app:speedLimitTextSize="7dp"
            app:speedDialRingWidth="7dp"
            app:speedDialRingInnerPadding="7dp"
            app:maxSpeed="10"
            android:layout_marginTop="8dp"
            app:layout_constraintTop_toBottomOf="@+id/textView6" app:layout_constraintStart_toStartOf="@+id/smRC"
    />
    <in.unicodelabs.kdgaugeview.KdGaugeView
            android:id="@+id/smBV"
            android:layout_width="100dp"
            android:layout_height="100dp"
            android:layout_gravity="center"
            app:speedTextSize="10dp"
            app:unitOfMeasurementTextSize="3dp"
            app:speedLimitTextSize="7dp"
            app:speedDialRingWidth="7dp"
            app:speedDialRingInnerPadding="7dp"
            app:maxSpeed="250"
            app:animationTime="5000"
            app:speed_limit="230"
            app:layout_constraintStart_toEndOf="@+id/smRV"
            app:layout_constraintTop_toTopOf="@+id/smRV"
            android:layout_marginEnd="8dp" app:layout_constraintEnd_toStartOf="@+id/smYV"
            android:layout_marginStart="8dp"/>

    <in.unicodelabs.kdgaugeview.KdGaugeView
            android:id="@+id/smRV"
            android:layout_width="100dp"
            android:layout_height="100dp"
            android:layout_gravity="center"
            app:speed_limit="238"
            app:speedTextSize="10dp"
            app:unitOfMeasurementTextSize="3dp"
            app:speedLimitTextSize="7dp"
            app:speedDialRingWidth="7dp"
            app:speedDialRingInnerPadding="7dp"
            app:maxSpeed="250"
            app:animationTime="5000"
            android:layout_marginTop="16dp"
            app:layout_constraintTop_toTopOf="parent" android:layout_marginStart="8dp"
            app:layout_constraintStart_toStartOf="parent"/>

    <in.unicodelabs.kdgaugeview.KdGaugeView
            android:layout_width="100dp"
            android:layout_height="100dp"
            android:layout_gravity="center"
            app:speed_limit="230"
            app:speedTextSize="10dp"
            app:unitOfMeasurementTextSize="3dp"
            app:speedLimitTextSize="7dp"
            app:speedDialRingWidth="7dp"
            app:speedDialRingInnerPadding="7dp"
            app:animationTime="5000"
