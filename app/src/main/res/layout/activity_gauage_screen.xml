<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout
        xmlns:android="http://schemas.android.com/apk/res/android"
        xmlns:tools="http://schemas.android.com/tools"
        xmlns:app="http://schemas.android.com/apk/res-auto"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        tools:context=".userinterface.GauageScreen">


    <in.unicodelabs.kdgaugeview.KdGaugeView
            android:id="@+id/smKVA2"
            android:layout_width="100dp"
            android:layout_height="100dp"
            android:layout_gravity="center"
            app:speed_limit="4"
            app:speedTextSize="10dp"
            app:unitOfMeasurementTextSize="3dp"
            app:speedLimitTextSize="7dp"
            app:speedDialRingWidth="7dp"
            app:speedDialRingInnerPadding="7dp"
            app:maxSpeed="10"
            app:animationTime="5000"
            app:unitOfMeasurement=""
            app:layout_constraintTop_toTopOf="@+id/smKVA1" app:layout_constraintStart_toEndOf="@+id/smKVA1"
